<UserControl x:Class="SysQ.Microgestion.Frontend.Common.Controls.TextBoxAutoCompleteProvider"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             Focusable="False" 
             IsTabStop="False"
             Width="Auto">
    <Popup x:Name="pop" Placement="Relative" HorizontalOffset="0"
           PopupAnimation="Slide">
        <ListBox x:Name="lstBox" SelectionMode="Single" SelectionChanged="lstBox_SelectionChanged">
            
        </ListBox>
    </Popup>
</UserControl>
